{
  "name": "alliance",
  "version": "0.0.1",
  "description": "dwd-project fof alliance business",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm run watch-client\" \"npm run start-server-dev\"",
    "watch-client": "better-npm-run watch-client",
    "start-server-dev": "better-npm-run start-server-dev",
    "build": "better-npm-run build",
    "lint": "eslint ./ --cache --fix --ignore-pattern .gitignore",
    "precommit-msg": "echo Pre-commit checking...please wait && exit 0"
  },
  "betterScripts": {
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000
      }
    },
    "start-server-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "keywords": [
    "alliance"
  ],
  "author": "frontend@dianwoda",
  "license": "MIT",
  "dependencies": {
    "antd": "^2.7.1",
    "babel-polyfill": "^6.22.0",
    "babel-register": "^6.22.0",
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.1",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "ejs": "^2.5.5",
    "express": "^4.14.1",
    "express-session": "^1.15.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "piping": "^1.0.0-rc.4",
    "react": "^15.4.2",
    "react-css-modules": "^4.1.0",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.1",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "redux-logger": "^2.0.0",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.3.2",
    "session-file-store": "^1.0.0",
    "uuid": "^3.0.1",
    "webpack": "^2.2.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-isomorphic-tools": "^2.6.6",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.4"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-import": "^1.1.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "better-npm-run": "0.0.14",
    "clean-webpack-plugin": "^0.1.15",
    "concurrently": "^3.3.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "ghooks": "^2.0.0",
    "install": "^0.8.7",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.0",
    "prepare-commit-msg-dianwoda": "^0.2.1",
    "sass-loader": "^6.0.0",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "validate-commit-msg": "^2.11.1"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "subject does not match subject pattern!",
      "helpMessage": ""
    },
    "ghooks": {
      "pre-commit": "npm run precommit-msg && npm run lint",
      "prepare-commit-msg": "prepare-commit-msg-dianwoda $2 $3",
      "commit-msg": "validate-commit-msg"
    }
  }
}
